    <CActorModel id="ShieldBatteryPlacementModel">
        <On Terms="ActorCreation" Target="::external.NexusShieldBatteryRangeIndicatorHelperList" Send="Signal ShieldBatteryPlacementOn"/>
        <On Terms="ActorDestruction" Target="::external.NexusShieldBatteryRangeIndicatorHelperList" Send="Signal ShieldBatteryPlacementOff"/>
        <On Terms="ActorCreation" Send="TimerSet 0.062500 NexusOverchargeSearch"/>
        <On Terms="TimerExpired; TimerName NexusOverchargeSearch" Send="TimerSet 0.062500 NexusOverchargeSearch"/>
        <On Terms="TimerExpired; TimerName NexusOverchargeSearch; !ValidatePosition NexusBatteryOvercharge8RangePlacementVisual" Send="StatusSet NexusOvercharge 0"/>
        <On Terms="TimerExpired; TimerName NexusOverchargeSearch; ValidatePosition NexusBatteryOvercharge8RangePlacementVisual" Send="StatusSet NexusOvercharge 1"/>
        <On Terms="StatusSet.*.NexusOvercharge; IsStatus NexusOvercharge 0; !Option Model Low" Send="ModelSwap ShieldBatteryPlacement"/>
        <On Terms="StatusSet.*.NexusOvercharge; IsStatus NexusOvercharge 1; !Option Model Low" Send="ModelSwap ShieldBattery_Placement_Overcharged"/>
        <On Terms="StatusSet.*.NexusOvercharge; IsStatus NexusOvercharge 0; Option Model Low" Send="SetTintColor 60,84,255 0.000000 OneShot OverchargeTint"/>
        <On Terms="StatusSet.*.NexusOvercharge; IsStatus NexusOvercharge 1; Option Model Low" Send="SetTintColor 238,255,21 0.000000 OneShot OverchargeTint"/>
    </CActorModel>
